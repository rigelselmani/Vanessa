<%- include('partials/Header') %>

<div class="container"style="margin-top:100px;">
  <div class="row">
    <%nails.forEach(function(nail){%>
      <div class="card col-md-4 my-1">
          <img style="height:250px; padding-top:10px;" src="<%= nail.image %>" alt="...">
          <div class="card-body">
            <h5 class="card-title"><%= nail.name %></h5>
            <p class="card-text"><%= nail.description %></p>
            <a href="/" class="btn btn-primary">More</a>
          </div>
      </div> 
    <%})%>   
  </div>
</div>

<%- include('partials/Footer') %>